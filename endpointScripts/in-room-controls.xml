<Extensions>
    <Version>1.4</Version>
    <Panel>
        <Icon>Handset</Icon>
        <Type>Statusbar</Type>
        <Page>
            <Name>Dial Pad</Name>
            <Row>
                <Name>Row</Name>
                <Widget>
                    <WidgetId>textBox</WidgetId>
                    <Name>Digits to dial</Name>
                    <Type>Text</Type>
                    <Options>size=3;align=center;fontSize=normal</Options>
                </Widget>
                <Widget>
                    <WidgetId>backSpace</WidgetId>
                    <Type>Button</Type>
                    <Options>size=1;icon=back</Options>
                </Widget>
                <Widget>
                    <WidgetId>Row_1</WidgetId>
                    <Type>GroupButton</Type>
                    <Options>size=4</Options>
                    <ValueSpace>
                        <Value>
                            <Key>1</Key>
                            <Name>1</Name>
                        </Value>
                        <Value>
                            <Key>2</Key>
                            <Name>2</Name>
                        </Value>
                        <Value>
                            <Key>3</Key>
                            <Name>3</Name>
                        </Value>
                    </ValueSpace>
                </Widget>
                <Widget>
                    <WidgetId>Row_2</WidgetId>
                    <Type>GroupButton</Type>
                    <Options>size=4</Options>
                    <ValueSpace>
                        <Value>
                            <Key>4</Key>
                            <Name>4</Name>
                        </Value>
                        <Value>
                            <Key>5</Key>
                            <Name>5</Name>
                        </Value>
                        <Value>
                            <Key>6</Key>
                            <Name>6</Name>
                        </Value>
                    </ValueSpace>
                </Widget>
                <Widget>
                    <WidgetId>Row_3</WidgetId>
                    <Type>GroupButton</Type>
                    <Options>size=4</Options>
                    <ValueSpace>
                        <Value>
                            <Key>7</Key>
                            <Name>7</Name>
                        </Value>
                        <Value>
                            <Key>8</Key>
                            <Name>8</Name>
                        </Value>
                        <Value>
                            <Key>9</Key>
                            <Name>9</Name>
                        </Value>
                    </ValueSpace>
                </Widget>
                <Widget>
                    <WidgetId>Row_4</WidgetId>
                    <Type>GroupButton</Type>
                    <Options>size=4</Options>
                    <ValueSpace>
                        <Value>
                            <Key>*</Key>
                            <Name>*</Name>
                        </Value>
                        <Value>
                            <Key>0</Key>
                            <Name>0</Name>
                        </Value>
                        <Value>
                            <Key>#</Key>
                            <Name>#</Name>
                        </Value>
                    </ValueSpace>
                </Widget>
                <Widget>
                    <WidgetId>dialButton</WidgetId>
                    <Name>Call</Name>
                    <Type>Button</Type>
                    <Options>size=4</Options>
                </Widget>
            </Row>
            <Options>hideRowNames=1</Options>
        </Page>
        <Name>Dial</Name>
    </Panel>
    <Panel>
        <Icon>Disc</Icon>
        <Type>Home</Type>
        <Page>
            <Name>Room Status</Name>
            <Row>
                <Name>Row</Name>
                <Widget>
                    <WidgetId>textBoxDND</WidgetId>
                    <Name>Room Available</Name>
                    <Type>Text</Type>
                    <Options>size=3;align=center</Options>
                </Widget>
                <Widget>
                    <WidgetId>DNDToggle</WidgetId>
                    <Type>ToggleButton</Type>
                    <Options>size=1</Options>
                </Widget>
            </Row>
            <Options>hideRowNames=1</Options>
        </Page>
        <Name>Room Availability</Name>
    </Panel>
</Extensions>